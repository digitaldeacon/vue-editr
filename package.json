{
  "name": "vue-editr",
  "version": "1.0.0",
  "description": "A WYSIWYG HTML editor for VueJS",
  "keywords": [
    "vue",
    "vue3",
    "wysiwyg",
    "html",
    "editor",
    "localization"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "vue-cli-service serve dev/serve.ts",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "lint": "vue-cli-service lint",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "main": "dist/vue-editr.ssr.js",
  "module": "dist/vue-editr.esm.js",
  "browser": "dist/vue-editr.esm.js",
  "unpkg": "dist/vue-editr.min.js",
  "files": [
    "dist/*",
    "vue-editr.d.ts",
    "src/**/*.vue"
  ],
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-replace": "^2.3.4",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "@vue/cli-plugin-babel": "^4.5.10",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-typescript": "^4.5.10",
    "@vue/cli-service": "^4.5.10",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "core-js": "^3.9.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.21.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^7.0.0-0",
    "minimist": "^1.2.5",
    "mitt": "^2.1.0",
    "postcss": "^8.2.3",
    "prettier": "^2.2.1",
    "rollup": "^2.36.1",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-sass": "^1.2.2",
    "rollup-plugin-scss": "^2.6.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "sass": "^1.32.8",
    "sass-loader": "^10.1.1",
    "ts-debounce": "^3.0.0",
    "typescript": "^4.2.3",
    "vue": "^3.0.5"
  },
  "peerDependencies": {
    "vue": "^3.0.5"
  },
  "engines": {
    "node": ">=12"
  },
  "sideEffects": false,
  "types": "vue-editr.d.ts"
}
